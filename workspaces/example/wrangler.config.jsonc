{
  // Some secrets written in .env will be added to this wrangler.config.jsonc
  // and wrangler.json will be generated.
  "$schema": "./node_modules/wrangler/config-schema.json",
  "name": "icedgate-example",
  "main": "src/index.tsx",
  "compatibility_date": "2024-11-13",
  "compatibility_flags": [
    "nodejs_compat"
  ],

  "route": {
    "pattern": "icedgate.phanective.org/*",
    "zone_name": "icedgate.phanective.org"
  },

  "d1_databases": [{
    "binding": "D1",
    "database_name": "icedgate-example",
    "preview_database_id": "local-icedgate-example"
    // Following properties are added from environmental variables
    // "database_id": "00000000-0000-0000-00000000",
  }],

  "env": {
    "staging": {
      "name": "icedgate-preview",
      "route": {
        "pattern": "icedgate-preview.phanective.org/*",
        "zone_name": "icedgate-preview.phanective.org"
      }
    }
  },

  "dev": {
    "port": 3000
  }
}
